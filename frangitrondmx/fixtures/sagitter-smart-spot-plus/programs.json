{
  "off": {
    "pan" : "$0",
    "tilt" : "$0",
    "pan_fine" : "$0",
    "tilt_fine" : "$0",
    "speed" : "$0",
    "gobo" : "$0",
    "dimmer" : "$0",
    "strobe" : "$0",
    "special" : "$0",
    "sensitivity" : "$0"
  },
  "build_1_a": {
    "pan" : "$60 if elapsed() % 4 < 0.7 else 110",
    "tilt" : "$220",
    "pan_fine" : "$0",
    "tilt_fine" : "$0",
    "speed" : "$0 if elapsed() % 4 < 0.7 else 235",
    "gobo" : "$16",
    "dimmer" : "$0 if elapsed() % 4 < 0.7 else 100",
    "strobe" : "$240",
    "special" : "$0",
    "sensitivity" : "$0"
  },
  "build_2_a": {
    "pan" : "$60 if elapsed() % 2 < 1 else 110",
    "tilt" : "$220",
    "pan_fine" : "$0",
    "tilt_fine" : "$0",
    "speed" : "$60",
    "gobo" : "$16",
    "dimmer" : "$100",
    "strobe" : "$240",
    "special" : "$0",
    "sensitivity" : "$0"
  },
  "scan_1_a": {
    "pan" : "$40 if elapsed() % 4 > 2 else 130",
    "tilt" : "$255 if elapsed() % 2 < 1 else 200",
    "pan_fine" : "$0",
    "tilt_fine" : "$0",
    "speed" : "$200",
    "gobo" : "$16",
    "dimmer" : "$100",
    "strobe" : "$0",
    "special" : "$0",
    "sensitivity" : "$0"
  },
  "scan_2_a": {
    "pan" : "$40 if elapsed() % 6 > 3 else 130",
    "tilt" : "$255",
    "pan_fine" : "$0",
    "tilt_fine" : "$0",
    "speed" : "$200",
    "gobo" : "$16",
    "dimmer" : "$100",
    "strobe" : "$0",
    "special" : "$0",
    "sensitivity" : "$0"
  },
  "white": {
    "color": "$0"
  },
  "red": {
    "color": "$11"
  },
  "blue": {
    "color": "$33"
  },
  "orange": {
    "color": "$55"
  },
  "blue_orange_2s": {
    "color": "$33 if elapsed() % 4 < 2 else 55"
  }
}
